{
  "validation": {
    "rules": {
      "systemInfoId": {
        "required": true
      },
      "name": {
        "required": true,
        "minlength": 4,
        "maxlength": 255
      }
    },
    "messages": {
      "systemInfoId": {
        "required": "系统信息标识必填"
      },
      "name": {
        "required": "系统信息名称必填",
        "minlength": "系统信息名称需{0}个字符以上",
        "maxlength": "系统信息名称需小于{0}个字符"
      }
    }
  }
}