{
  "validation": {
    "rules": {
      "variableId": {
        "required": true
      },
      "name": {
        "required": true,
        "minlength": 4,
        "maxlength": 255
      }
    },
    "messages": {
      "variableId": {
        "required": "变量标识必填"
      },
      "name": {
        "required": "变量名称必填",
        "minlength": "变量名称需{0}个字符以上",
        "maxlength": "变量名称需小于{0}个字符"
      }
    }
  }
}