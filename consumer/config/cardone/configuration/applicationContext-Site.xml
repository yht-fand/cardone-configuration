<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">
    <!-- Site -->
    <bean name="top.cardone.configuration.service.SiteService" parent="invokerProxyFactoryParnet">
        <property name="serviceInterface" value="top.cardone.configuration.service.SiteService"/>
        <property name="queue">
            <bean class="org.apache.activemq.command.ActiveMQQueue">
                <constructor-arg value="cardone-configuration-${cardone-configuration-version}:top.cardone.configuration.service.SiteService"/>
            </bean>
        </property>
    </bean>
</beans>