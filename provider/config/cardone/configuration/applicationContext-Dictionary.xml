<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">
    <!-- 字典 -->
    <bean id="top.cardone.configuration.dao.DictionaryDao"
          class="top.cardone.configuration.dao.impl.DictionaryDaoImpl">
        <property name="configTable">
            <map>
                <entry key="config">
                    <map>
                        <entry key="sqlFileRoot" value="/cardone/configuration/dictionary/"/>
                        <entry key="pks" value="dictionaryId"/>
                        <entry key="codes" value="systemInfoCode,siteCode,orgCode,dictionaryTypeCode,dictionaryCode,endDate"/>
                    </map>
                </entry>
                <entry key="insert">
                    <map>
                        <entry key="dictionaryId" value="readRandomUUIDFunc"/>
                        <entry key="createdByCode" value="readPrincipalFunc"/>
                        <entry key="createdDate" value="readOneDateFunc"/>
                        <entry key="beginDate" value="readOneDateFunc"/>
                        <entry key="endDate" value="endDateDefaultValueFunc"/>
                        <entry key="systemInfoCode" value="systemInfoCodeDefaultValueFunc"/>
                        <entry key="siteCode" value="siteCodeDefaultValueFunc"/>
                        <entry key="orgCode" value="orgCodeDefaultValueFunc"/>
                    </map>
                </entry>
                <entry key="update">
                    <map>
                        <entry key="lastModifiedByCode" value="readPrincipalFunc"/>
                        <entry key="lastModifiedDate" value="readOneDateFunc"/>
                        <entry key="endDate" value="endDateDefaultValueFunc"/>
                        <entry key="systemInfoCode" value="systemInfoCodeDefaultValueFunc"/>
                        <entry key="siteCode" value="siteCodeDefaultValueFunc"/>
                        <entry key="orgCode" value="orgCodeDefaultValueFunc"/>
                    </map>
                </entry>
            </map>
        </property>
    </bean>
	
    <bean id="top.cardone.configuration.service.DictionaryService"
          class="top.cardone.configuration.service.impl.DictionaryServiceImpl">
		<property name="dao" ref="top.cardone.configuration.dao.DictionaryDao"/>
	</bean>
</beans>