<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">
    <bean name="dictionarySelectFunc"
          class="top.cardone.data.func.FindListFunc">
        <property name="serviceBeanName" value="top.cardone.configuration.service.DictionaryService"/>
    </bean>

    <bean name="readOneDictionaryFunc"
          class="top.cardone.data.func.ReadOneFunc">
        <property name="serviceBeanName" value="top.cardone.configuration.service.DictionaryService"/>
    </bean>

    <bean name="readOneDictionaryByDictionaryTypeCodesFunc"
          class="top.cardone.configuration.func.ReadOneDictionaryByDictionaryTypeCodesFunc"/>

    <bean name="dictionaryTypeSelectFunc"
          class="top.cardone.data.func.FindListFunc">
        <property name="serviceBeanName" value="top.cardone.configuration.service.DictionaryTypeService"/>
    </bean>

    <bean name="readOneDictionaryTypeFunc"
          class="top.cardone.data.func.ReadOneFunc">
        <property name="serviceBeanName" value="top.cardone.configuration.service.DictionaryTypeService"/>
    </bean>

    <bean name="readOneErrorInfoContentFunc" class="top.cardone.configuration.func.ReadOneErrorInfoContentFunc"/>

    <bean id="generateDictionaryTypeTreeFunc"
          class="top.cardone.data.func.GenerateTreeFunc">
        <property name="codePutKey" value="dictionaryTypeCode"/>
        <property name="codeGetKey" value="DICTIONARY_TYPE_CODE"/>
        <property name="pkPutKey" value="dictionaryTypeId"/>
        <property name="pkGetKey" value="DICTIONARY_TYPE_ID"/>
        <property name="service" ref="top.cardone.configuration.service.DictionaryTypeService"/>
    </bean>

    <bean id="generateNavigationTreeFunc"
          class="top.cardone.data.func.GenerateTreeFunc">
        <property name="codePutKey" value="navigationCode"/>
        <property name="codeGetKey" value="NAVIGATION_CODE"/>
        <property name="pkPutKey" value="navigationId"/>
        <property name="pkGetKey" value="NAVIGATION_ID"/>
        <property name="service" ref="top.cardone.configuration.service.NavigationService"/>
    </bean>
</beans>